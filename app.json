{
  "expo": {
    "name": "GeoLink",
    "slug": "geolink",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "GeoLink is a mobile app that allows users to connect to their friends using Bluetooth, and share messages via the GeoLink grid.",
    "githubUrl": "https://github.com/diragb/geolink-expo"
  }
}
